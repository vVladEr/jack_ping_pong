class Vector2{

    field int x, y;

    constructor Vector2 new(int inputX, int inputY)
    {
        let x = inputX;
        let y = inputY;
        return this;
    }

    method int getX() {
        return x;
    }

    method int getY(){
        return y;
    }

    method void add(Vector2 b)
    {
        let x = x + b.getX();
        let y = y + b.getY();
    }

    method void dispose(){
        do Memory.deAlloc(this);
        return;
    }

}